<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="davinci.controllers.SecondaryController">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="466.0" text="Student Account Creator" textAlignment="CENTER">
         <font>
            <Font name="Goudy Old Style" size="37.0" />
         </font></Label>
      <TextField id="accountCreatorUsernameText" maxWidth="200.0" promptText="username" />
      <TextField id="accountCreatorPasswordText" layoutX="310.0" layoutY="201.0" maxWidth="200.0" promptText="password" />
      <TextField id="accountCreatorFirstNameTest" layoutX="310.0" layoutY="374.0" maxWidth="200.0" promptText="first name" />
      <TextField id="accountCreatorLastNameText" layoutX="310.0" layoutY="396.0" maxWidth="200.0" promptText="last name" />
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="create account" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
